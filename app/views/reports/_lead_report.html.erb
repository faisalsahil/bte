<%= link_to 'download pdf', '#', class:'pull-right btn btn-mini btn-success' %>
<!--generate_report_reports_path({from_date: @from_date || '', to_date: @to_date || '', report_type: @type, format: :pdf})-->
<table class="table table-striped">
  <thead>
  <th>Area</th>
  <th>code | name</th>
  <th>Contact name</th>
  <th>Contact #</th>
  <th>Sale representative</th>
  <th>Oil used</th>
  <th>Rate/Kg</th>
  <th>Address</th>
  </thead>
  <tbody>
    <% @branches&.each do |branch| %>
        <tr>
          <td><%= branch.area.name %></td>
          <td><%= "#{branch.branch_code} | #{branch.branch_name}"  %></td>
          <td><%= branch.contact_name %></td>
          <td><%= branch.contact_phone %></td>
          <td><%= branch.try(:representator).try(:name) || 'N/A' %></td>
          <td><%= branch.monthly_oil_used %></td>
          <td><%= branch.rate_per_kg %></td>
          <td><%= branch.address %></td>
        </tr>
    <% end %>
  </tbody>
</table>