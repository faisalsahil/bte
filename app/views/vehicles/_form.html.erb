<%= simple_form_for(@vehicle) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :vehicle_type %>
    <%= f.input :vehicle_number %>
  </div>

  <div class="form-actions">
    <%= link_to 'Back', vehicles_path, class: 'btn btn-mini btn-default' %>
    <%= f.button :submit, class: 'btn btn-mini btn-primary' %>
  </div>
<% end %>
