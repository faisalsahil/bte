<% if @current_user_site != nil && @current_user_role == AppConstants::PFA %>
    <%= render 'site_dashboard' %>
<% elsif @current_user_role == AppConstants::PFA %>
    <%= render 'pfa_dashboard' %>
<% elsif @current_user_role == AppConstants::SUPER_ADMIN %>
    <%= render 'super_admin_dashboard' %>
<% else %>
    <%= render 'site_dashboard' %>
<% end %>